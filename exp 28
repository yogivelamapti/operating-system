#include <stdio.h>
#include <string.h>

int main() {
    char pattern[50], filename[20], line[200];
    FILE *fp;

    printf("Enter search pattern: ");
    scanf("%s", pattern);

    printf("Enter filename: ");
    scanf("%s", filename);

    fp = fopen(filename, "r");
    if (!fp) {
        printf("File not found\n");
        return 0;
    }

    while (fgets(line, sizeof(line), fp)) {
        if (strstr(line, pattern))
            printf("%s", line);
    }

    fclose(fp);
    return 0;
}
