#include <stdio.h>

int main() {
    int indexBlock, n;

    printf("Enter index block number: ");
    scanf("%d", &indexBlock);

    printf("Enter number of blocks for the file: ");
    scanf("%d", &n);

    int blocks[n];

    printf("Enter block numbers: ");
    for (int i = 0; i < n; i++)
        scanf("%d", &blocks[i]);

    printf("\nIndex Block %d contains:\n", indexBlock);

    for (int i = 0; i < n; i++)
        printf("Block %d\n", blocks[i]);

    return 0;
}
