#include <stdio.h>

int predict(int pages[], int frame[], int n, int index, int frames) {
    int farthest = index, pos = -1;

    for (int i = 0; i < frames; i++) {
        int j;
        for (j = index; j < n; j++) {
            if (frame[i] == pages[j]) {
                if (j > farthest) {
                    farthest = j;
                    pos = i;
                }
                break;
            }
        }
        if (j == n)
            return i;
    }
    return (pos == -1) ? 0 : pos;
}

int main() {
    int frames, pages;

    printf("Enter number of frames: ");
    scanf("%d", &frames);

    printf("Enter number of pages: ");
    scanf("%d", &pages);

    int page[pages], frame[frames];
    printf("Enter page reference string:\n");

    for (int i = 0; i < pages; i++)
        scanf("%d", &page[i]);

    for (int i = 0; i < frames; i++)
        frame[i] = -1;

    int faults = 0;

    for (int i = 0; i < pages; i++) {
        int hit = 0;

        for (int j = 0; j < frames; j++) {
            if (frame[j] == page[i]) {
                hit = 1;
                break;
            }
        }

        if (!hit) {
            int pos = -1;

            for (int j = 0; j < frames; j++) {
                if (frame[j] == -1) {
                    pos = j;
                    break;
                }
            }

            if (pos == -1)
                pos = predict(page, frame, pages, i + 1, frames);

            frame[pos] = page[i];
            faults++;
        }

        printf("\nFrames: ");
        for (int j = 0; j < frames; j++)
            printf("%d ", frame[j]);
    }

    printf("\n\nTotal Page Faults = %d\n", faults);
    return 0;
}
