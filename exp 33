#include <stdio.h>

int main() {
    int start, length, block;

    printf("Enter starting block: ");
    scanf("%d", &start);

    printf("Enter file length: ");
    scanf("%d", &length);

    printf("Allocated blocks:\n");

    for (int i = start; i < start + length; i++)
        printf("%d ", i);

    printf("\nSequential allocation completed.\n");
    return 0;
}
