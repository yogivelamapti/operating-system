#include <stdio.h>
#include <stdlib.h>

int main() {
    int n,h,d,a[50],t=0; 
scanf("%d",&n);
    for(int i=0;i<n;i++) 
scanf("%d",&a[i]);
    scanf("%d%d",&h,&d);
    for(int i=0;i<n-1;i++) 
for(int j=i+1;j<n;j++)
if(a[i]>a[j]){int x=a[i];a[i]=a[j];a[j]=x;}
    for(int i=0;i<n;i++) 
if(a[i]>=h){
for(int j=i;j<n;j++) t+=abs(a[j]-h),h=a[j]; t+=abs((d-1)-h); h=d-1; for(int j=i-1;j>=0;j--) t+=abs(a[j]-h),h=a[j]; break;}
    printf("%d",t);
}
#INPUT
Requests: 98 183 37 122 14 124 65 67
Head: 53
Disk Size: 200
Direction: 1 (right)
#OUTPUT
Total Head Movement = 331
